{
    "extends": [
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],

    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "semi": [2, "always"],
      "no-console": "off",
      "no-debugger": "off",
      "no-await-in-loop": "off",
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "prettier/prettier": [
        "error", {
          "singleQuote": true,
          "semi": true,
          "endOfLine":"auto"
        }
      ]
    }
  }